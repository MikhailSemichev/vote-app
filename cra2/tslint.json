{
    "extends": [
        "tslint:latest",
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "rulesDirectory": ["tslint-plugin-prettier"],
    "rules": {
        "prettier": true,
        "interface-name": true,
        "ordered-imports": false,
        "member-access": false,
        "no-console": false,
        "object-literal-sort-keys": false
    }
}
